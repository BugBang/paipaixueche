package com.session.dgjp;

import com.session.common.BaseActivity;
import com.session.dgjp.view.CarouselView;

import android.os.Bundle;

public class HomePagerActivity extends BaseActivity{
	
	private CarouselView mCarouselView;

	@Override
	protected void init(Bundle savedInstanceState) {
		setContentView(R.layout.act_home_pager);
		initView();
	}

	private void initView() {
		mCarouselView = (CarouselView) findViewById(R.id.top_ad_view);
	}
}
